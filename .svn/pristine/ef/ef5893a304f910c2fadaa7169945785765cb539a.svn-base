* { margin:0; padding:0}

body { font-family:Arial, Helvetica, sans-serif; font-size:12px; background:#fafafa;}
 
ul li { list-style-type:none; }
h1{font-size:16px; padding:0 0 5px 0; margin:0 0; font-weight:normal; color:#606060;}
h2{font-size:18px; padding:0 0 5px 0; margin:0 0; font-weight:normal; color:#ed5a48;}
h3{font-size:24px; padding:0 0 5px 0; margin:0 0; font-weight:normal; border-bottom:1px solid #e8e8e8; color:#ed5a48;}
h6{font-size:45px; padding:0 0 5px 0; margin:50px 0 0 0; font-weight:normal; color:#434343; text-align:center;}

.wrapper { width:960px; margin:0 auto; }
.header { border-radius:0 0 5px 5px; background:#ed5a48  }
.clear{clear:both; margin:0px; padding:0px; height:0px;}
.clear10{clear:both; margin:0px; padding:0px; height:10px;}
.clear20{clear:both; margin:0px; padding:0px; height:20px;}

.header .c1{float:left; width:740px;}
.header .c2{float:left; width:200px; text-align:right; font-size:12px; color:#ffffff; padding:12px 0;}
.header .c2 a{color:#000000; text-decoration:none}
.header .c1 .nav  li {float:left; padding:10px 15px; background:url(../images/nav-divider.jpg) 0 0 repeat-y;}
.header .c1 .nav  li:hover, .header .c1 .nav  li.active{background:#d64634;}
 
.header .c1 .nav  li.first{background:url(../images/sprit.png) 10px 8px no-repeat; text-indent:-9999999px; width: 15px; cursor: pointer;}
.header .c1 .nav  li a{text-decoration:none;  padding:0px; font-size:16px; color:#ffffff;}

.container{ border-radius:5px; background:#ffffff; border:solid 1px #e8e8e8; padding:20px; }
.container .c1{float:left; margin-right:20px;}
.container .c2{float:left;}
.container .c1.dashboard{width:590px;}
.container .c2.news{width:308px;}
.container .c1.tender-detail{width:420px;}
.container .c2.tender-grid{width:478px;}
.container .c2.news h3{background:url(../images/sprit.png) 0 -42px no-repeat; padding-left: 43px;}

.table{border:0px; padding:0px;}
.table table{border-bottom:1px solid #e8e8e8; border-left:1px solid #e8e8e8;}
.table table td{border-top:1px solid #e8e8e8; border-right:1px solid #e8e8e8; padding:8px; text-align:left;}
.table table th{border-top:1px solid #e8e8e8; border-right:1px solid #e8e8e8; font-weight:bold; background:#5f5f5f; color:#ffffff; padding:8px; text-align:left;}
.table table .total{background:#f7f7f7; color:#5f5f5f; font-weight:bold;}

.news .news-row{float:left; margin:0px; padding:10px 0; border-bottom:1px solid #e8e8e8; }
.news .news-row .date{float:left; margin:0px 15px 0 0; padding:0px; width:50px}
.news .news-row .date .numeric{font-size:30px; color:#717171;}
.news .news-row .date .numeric span{font-size:12px; vertical-align:top;}
.news .news-row .date .month{font-size:10px;} 
.news .news-row .news{float:left; margin:0px; padding:0px; width:243px;}

.heading{color:#797979; font-size:30px; font-weight:normal; text-align:center;}
.sub-heading{font-size:20px; color:#434343; font-weight:normal; text-align:center; line-height: 36px;}
.input{border:1px solid #e8e8e8; -moz-border-radius:3px; -webkit-border-radius:3px; border-radius:3px; padding:5px;}
.btn-01{background:#ed5a48; color:#ffffff; padding:8px 15px; margin-right:10px; text-decoration:none; -moz-border-radius:3px; -webkit-border-radius:3px; border-radius:3px; font-size:14px;}
.btn-02{background:#ed5a48; color:#ffffff; padding:10px 17px; margin-right:10px; text-decoration:none; -moz-border-radius:3px; -webkit-border-radius:3px; border-radius:3px; font-size:14px;}

.login-box{border:solid 1px #bebebe; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius:5px; width:350px; margin:0 auto; padding:40px;}
.login-box .label{color:#797979; font-size:14px;}
.login-box .field {margin-top:10px;}
.login-box .field input{border:1px solid #bebebe; padding:7px; width: 330px; -moz-border-radius:3px; -webkit-border-radius:3px; border-radius:3px; font-size:14px;}

.grid-menu{margin:0px; display:block; padding:0px;}
.grid-menu li{float:left; margin-right:10px;  -moz-border-radius:3px; -webkit-border-radius:3px; border-radius:3px; border:solid 1px #e8e8e8; background:#f7f7f7; padding: 5px 13px;}
.grid-menu li:hover, .grid-menu li.active{background:#ffffff;}
.grid-menu li a{text-decoration:none; color:#5f5f5f; font-size:14px;}

.form-wrapper{width:100%; margin:0px; padding:0px; font-size:14px;}
.form-wrapper .form-row{float:left; margin:0px 0 20px 0; padding:0px}
.form-wrapper .form-row .label{float:left; width:300px; text-align:right; margin-right:15px;}
.form-wrapper .form-row .field{float:left; width:603px;}
.form-wrapper .form-row .field .input{border:1px solid #bebebe; padding:5px; width:330px; border-radius:0px;}
.form-wrapper .form-row .field .select{border:1px solid #bebebe; padding:5px; width:330px;}
.form-wrapper .form-row .field .date{border:1px solid #bebebe; padding:5px; width:120px; float:left}
.calendar{background:url(../images/sprit.png) 3px -84px no-repeat; text-indent:-9999999px; width: 46px; cursor: pointer; float: left; height: 28px; }

 
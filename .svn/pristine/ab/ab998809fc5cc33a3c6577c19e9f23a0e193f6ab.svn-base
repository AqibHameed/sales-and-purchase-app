class News < ActiveRecord::Base

  attr_accessible :title, :url, :date, :category, :description, :status

  rails_admin do
    edit do
      field :title
      field :url
      field :date
      field :category, :enum do
        enum do
          ['News', 'Event']
        end
      end
      field :description
      field :status
    end
  end

end

